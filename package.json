{
    "name": "legodb",
    "private": true,
    "scripts": {
        "commit": "cz-customizable",
        "build": "turbo build",
        "dev": "turbo dev",
        "dev:old-web": "turbo dev --filter=@legodb/old-web",
        "dev:web": "turbo dev --filter=@legodb/web",
        "dev:backend": "turbo dev --filter=@legodb/backend",
        "db:push": "turbo db:push",
        "db:generate": "turbo db:generate",
        "db:push-web": "turbo db:push-web",
        "db:generate-web": "turbo db:generate-web",
        "lint": "turbo lint",
        "format": "prettier --write \"**/*.{ts,tsx,md}\""
    },
    "devDependencies": {
        "prettier": "^3.2.5",
        "turbo": "^2.3.2",
        "typescript": "5.5.4",
        "cz-customizable": "^7.0.0"
    },
    "packageManager": "pnpm@9.0.0",
    "engines": {
        "node": ">=18"
    }
}